{
  "mcpServers": {
    "puppeteer": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-puppeteer"
      ],
      "env": {}
    },
    "fetch": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "mcp-server-fetch"
      ],
      "env": {}
    },
    "web-browser": {
      "type": "stdio",
      "command": "uv",
      "args": [
        "tool",
        "run",
        "web-browser-mcp-server"
      ],
      "env": {}
    }
  }
}